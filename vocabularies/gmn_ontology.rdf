@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix cidoc: <http://www.cidoc-crm.org/cidoc-crm/> .
@prefix gmn: <http://www.genoesemerchantnetworks.com/ontology#> .

# Ontology Declaration
<http://www.genoesemerchantnetworks.com/ontology> 
    a owl:Ontology ;
    rdfs:label "Genoese Merchant Networks CIDOC-CRM Extension"@en ;
    rdfs:comment "Custom vocabulary extending CIDOC-CRM for the Genoese Merchant Networks project. Provides simplified shortcut properties and classes that represent more complex CIDOC-CRM paths."@en ;
    dcterms:title "GMN CIDOC-CRM Extension Ontology"@en ;
    dcterms:creator "Genoese Merchant Networks Project" ;
    dcterms:created "2025-10-16"^^xsd:date ;
    dcterms:modified "2025-10-16"^^xsd:date ;
    owl:versionInfo "1.0" ;
    owl:imports <http://www.cidoc-crm.org/cidoc-crm/> .

# Class: E74.1 Regional Provenance
gmn:E74_1_Regional_Provenance
    a owl:Class ;
    rdfs:subClassOf cidoc:E74_Group ;
    rdfs:label "E74.1 Regional Provenance"@en ;
    rdfs:comment "Simplified class that describes the regional provenance of a person in terms of a type/place value pair. This is a specialized type of E74_Group used to represent regional or geographic origins, citizenship, or cultural affiliations. Instances of this class typically combine a group name (via P1 properties) with a place reference (via P74_has_current_or_former_residence) to capture statements like 'citizens of Genoa' or 'Genoese merchants'."@en ;
    dcterms:created "2025-10-16"^^xsd:date .

# Class: E74.2 Social Category
gmn:E74_2_Social_Category
    a owl:Class ;
    rdfs:subClassOf cidoc:E74_Group ;
    rdfs:label "E74.2 Social Category"@en ;
    rdfs:comment "Simplified class that describes the social category of a person in terms of a type/place value pair. This is a specialized type of E74_Group used to represent social classes, occupational groups, professional associations, or other social categorizations. Instances of this class capture social affiliations such as 'merchants', 'nobility', 'guild members', etc."@en ;
    dcterms:created "2025-10-16"^^xsd:date .

# Class: E74.3 Occupational Group
gmn:E74_3_Occupational_Group
    a owl:Class ;
    rdfs:subClassOf cidoc:E74_Group ;
    rdfs:label "E74.3 Occupational Group"@en ;
    rdfs:comment "Simplified class that describes professional or occupational groups. This is a specialized type of E74_Group used to represent professions, trades, or occupational categories. Instances of this class capture professional affiliations such as 'notaries', 'merchants', 'bankers', 'ship captains', etc. Membership in these groups indicates the person practiced or was associated with that profession."@en ;
    dcterms:created "2025-10-16"^^xsd:date .

# Property: P1.1 has name
gmn:P1_1_has_name 
    a owl:DatatypeProperty ;
    a rdf:Property ;
    rdfs:label "P1.1 has name"@en ;
    rdfs:comment "Simplified property for expressing the name of a person. Represents the full CIDOC-CRM path: P1_is_identified_by > E41_Appellation > P2_has_type <http://vocab.getty.edu/page/aat/300458798> > P190_has_symbolic_content. This property is provided as a convenience for data entry and should be transformed to the full CIDOC-CRM structure for formal compliance. The appellation type is automatically set to AAT 300458798 (personal names)."@en ;
    rdfs:subPropertyOf cidoc:P1_is_identified_by ;
    rdfs:domain cidoc:E21_Person ;
    rdfs:range cidoc:E62_String ;
    dcterms:created "2025-10-16"^^xsd:date ;
    rdfs:seeAlso cidoc:P1_is_identified_by, cidoc:P190_has_symbolic_content, <http://vocab.getty.edu/page/aat/300458798> ;
    owl:equivalentProperty [
        a owl:Restriction ;
        owl:onProperty cidoc:P1_is_identified_by ;
        owl:allValuesFrom [
            a owl:Restriction ;
            owl:onProperty cidoc:P190_has_symbolic_content ;
            owl:hasValue cidoc:E62_String
        ]
    ] ;
    gmn:hasImplicitType <http://vocab.getty.edu/page/aat/300458798> .

# Property: P1.2 has name from source
gmn:P1_2_has_name_from_source 
    a owl:DatatypeProperty ;
    a rdf:Property ;
    rdfs:label "P1.2 has name from source"@en ;
    rdfs:comment "Simplified property for expressing the name of a person as found in historical sources. Represents the full CIDOC-CRM path: P1_is_identified_by > E41_Appellation > P2_has_type <http://vocab.getty.edu/page/aat/300456607> > P190_has_symbolic_content. This property is provided as a convenience for data entry and should be transformed to the full CIDOC-CRM structure for formal compliance. The appellation type is automatically set to AAT 300456607 (names found in historical sources)."@en ;
    rdfs:subPropertyOf cidoc:P1_is_identified_by ;
    rdfs:domain cidoc:E21_Person ;
    rdfs:range cidoc:E62_String ;
    dcterms:created "2025-10-16"^^xsd:date ;
    rdfs:seeAlso cidoc:P1_is_identified_by, cidoc:P190_has_symbolic_content, <http://vocab.getty.edu/page/aat/300456607> ;
    owl:equivalentProperty [
        a owl:Restriction ;
        owl:onProperty cidoc:P1_is_identified_by ;
        owl:allValuesFrom [
            a owl:Restriction ;
            owl:onProperty cidoc:P190_has_symbolic_content ;
            owl:hasValue cidoc:E62_String
        ]
    ] ;
    gmn:hasImplicitType <http://vocab.getty.edu/page/aat/300456607> .

# Property: P3.1 has editorial note
gmn:P3_1_has_editorial_note 
    a owl:DatatypeProperty ;
    a rdf:Property ;
    rdfs:label "P3.1 has editorial note"@en ;
    rdfs:comment "Simplified property for expressing editorial notes, comments, and internal documentation about an entity. Represents the full CIDOC-CRM path: P67i_is_referred_to_by > E33_Linguistic_Object > P2_has_type <http://vocab.getty.edu/page/aat/300456627> > P190_has_symbolic_content. This property is provided as a convenience for data entry and should be transformed to the full CIDOC-CRM structure for formal compliance. The linguistic object type is automatically set to AAT 300456627. This property is intended for internal project use and may be excluded from public data exports."@en ;
    rdfs:subPropertyOf cidoc:P3_has_note ;
    rdfs:domain cidoc:E1_CRM_Entity ;
    rdfs:range cidoc:E62_String ;
    dcterms:created "2025-10-16"^^xsd:date ;
    rdfs:seeAlso cidoc:P3_has_note, cidoc:P67i_is_referred_to_by, cidoc:P190_has_symbolic_content, <http://vocab.getty.edu/page/aat/300456627> ;
    gmn:hasImplicitType <http://vocab.getty.edu/page/aat/300456627> ;
    gmn:isInternalOnly true .

# Property: P2.1 has sex/gender
gmn:P2_1_gender
    a owl:ObjectProperty ;
    a rdf:Property ;
    rdfs:label "P2.1 has sex/gender"@en ;
    rdfs:comment "Simplified property for expressing biological characteristics and physiological traits that distinguish the males and females of a species. This is an extension of the CIDOC-CRM path: E21_Person > P2_has_type > E55_Type. The range is restricted to a controlled vocabulary of three Getty AAT terms: male (300189559), female (300189557), and intersex (300417544)."@en ;
    rdfs:subPropertyOf cidoc:P2_has_type ;
    rdfs:domain cidoc:E21_Person ;
    rdfs:range [
        a owl:Class ;
        owl:oneOf (
            <http://vocab.getty.edu/page/aat/300189559>
            <http://vocab.getty.edu/page/aat/300189557>
            <http://vocab.getty.edu/page/aat/300417544>
        )
    ] ;
    dcterms:created "2025-10-16"^^xsd:date ;
    rdfs:seeAlso cidoc:P2_has_type, <http://vocab.getty.edu/page/aat/300189559>, <http://vocab.getty.edu/page/aat/300189557>, <http://vocab.getty.edu/page/aat/300417544> .

# Property: P1.4 has loconym
gmn:P1_4_has_loconym
    a owl:ObjectProperty ;
    a rdf:Property ;
    rdfs:label "P1.4 has loconym"@en ;
    rdfs:comment "Simplified property for expressing a place referenced in a person's name (loconym), indicating that the person or their ancestors originated from that place. Represents the full CIDOC-CRM path: P1_is_identified_by > E41_Appellation > P2_has_type <https://www.wikidata.org/wiki/Q17143070> > P67_refers_to > E53_Place. This captures toponymic naming patterns common in medieval Italian contexts (e.g., 'Giovanni da Genova', 'Bartolomeo de Vignolo'). This property is provided as a convenience for data entry and should be transformed to the full CIDOC-CRM structure for formal compliance. The appellation type is automatically set to Wikidata Q17143070 (loconym)."@en ;
    rdfs:subPropertyOf cidoc:P1_is_identified_by ;
    rdfs:domain cidoc:E21_Person ;
    rdfs:range cidoc:E53_Place ;
    dcterms:created "2025-10-16"^^xsd:date ;
    rdfs:seeAlso cidoc:P1_is_identified_by, cidoc:P67_refers_to, <https://www.wikidata.org/wiki/Q17143070> ;
    gmn:hasImplicitType <https://www.wikidata.org/wiki/Q17143070> .

# Property: P11i.1 earliest attestation date
gmn:P11i_1_earliest_attestation_date
    a owl:DatatypeProperty ;
    a rdf:Property ;
    rdfs:label "P11i.1 earliest attestation date"@en ;
    rdfs:comment "Simplified property for expressing the earliest date at which a person is documented or attested to have been alive in historical sources. Represents the full CIDOC-CRM path: E21_Person > P11i_participated_in > E5_Event > P4_has_time-span > E52_Time-Span > P82a_begin_of_the_begin. This property captures the earliest known documentary evidence of a person's existence. This property is provided as a convenience for data entry and should be transformed to the full CIDOC-CRM structure for formal compliance."@en ;
    rdfs:subPropertyOf cidoc:P11i_participated_in ;
    rdfs:domain cidoc:E21_Person ;
    rdfs:range xsd:date ;
    dcterms:created "2025-10-16"^^xsd:date ;
    rdfs:seeAlso cidoc:P11i_participated_in, cidoc:P4_has_time-span, cidoc:P82a_begin_of_the_begin .

# Property: P11i.2 latest attestation date
gmn:P11i_2_latest_attestation_date
    a owl:DatatypeProperty ;
    a rdf:Property ;
    rdfs:label "P11i.2 latest attestation date"@en ;
    rdfs:comment "Simplified property for expressing the latest date at which a person is documented or attested to have been alive in historical sources. Represents the full CIDOC-CRM path: E21_Person > P11i_participated_in > E5_Event > P4_has_time-span > E52_Time-Span > P82b_end_of_the_end. This property captures the last known documentary evidence of a person's existence. This property is provided as a convenience for data entry and should be transformed to the full CIDOC-CRM structure for formal compliance."@en ;
    rdfs:subPropertyOf cidoc:P11i_participated_in ;
    rdfs:domain cidoc:E21_Person ;
    rdfs:range xsd:date ;
    dcterms:created "2025-10-16"^^xsd:date ;
    rdfs:seeAlso cidoc:P11i_participated_in, cidoc:P4_has_time-span, cidoc:P82b_end_of_the_end .

# Property: P11i.3 has spouse
gmn:P11i_3_has_spouse
    a owl:ObjectProperty ;
    a rdf:Property ;
    rdfs:label "P11i.3 has spouse"@en ;
    rdfs:comment "Simplified property for expressing a spousal relationship between two persons. Represents the full CIDOC-CRM path: E21_Person > P11i_participated_in > E5_Event > P2_has_type <http://vocab.getty.edu/page/aat/300055475> > P11_had_participant > E21_Person. This property captures marriage relationships through a marriage event. This property is provided as a convenience for data entry and should be transformed to the full CIDOC-CRM structure for formal compliance. The event type is automatically set to AAT 300055475 (marriages)."@en ;
    rdfs:subPropertyOf cidoc:P11i_participated_in ;
    rdfs:domain cidoc:E21_Person ;
    rdfs:range cidoc:E21_Person ;
    dcterms:created "2025-10-16"^^xsd:date ;
    rdfs:seeAlso cidoc:P11i_participated_in, cidoc:P11_had_participant, <http://vocab.getty.edu/page/aat/300055475> ;
    gmn:hasImplicitType <http://vocab.getty.edu/page/aat/300055475> .

# Property: P107i.1 has regional provenance
gmn:P107i_1_has_regional_provenance
    a owl:ObjectProperty ;
    a rdf:Property ;
    rdfs:label "P107i.1 has regional provenance"@en ;
    rdfs:comment "Simplified property associating a person with a regional provenance attributed to that person. Represents the full CIDOC-CRM path: E21_Person > P107i_is_current_or_former_member_of > E74_Group > P2_has_type <http://vocab.getty.edu/page/aat/300055490>. This captures regional, geographic, or cultural group membership. This property is provided as a convenience for data entry and should be transformed to the full CIDOC-CRM structure for formal compliance. The group type is automatically set to AAT 300055490 (regions (geographic)). The range is restricted to gmn:E74_1_Regional_Provenance groups."@en ;
    rdfs:subPropertyOf cidoc:P107i_is_current_or_former_member_of ;
    rdfs:domain cidoc:E21_Person ;
    rdfs:range gmn:E74_1_Regional_Provenance ;
    dcterms:created "2025-10-16"^^xsd:date ;
    rdfs:seeAlso cidoc:P107i_is_current_or_former_member_of, <http://vocab.getty.edu/page/aat/300055490> ;
    gmn:hasImplicitType <http://vocab.getty.edu/page/aat/300055490> .

# Property: P107i.2 has social category
gmn:P107i_2_has_social_category
    a owl:ObjectProperty ;
    a rdf:Property ;
    rdfs:label "P107i.2 has social category"@en ;
    rdfs:comment "Simplified property associating a person with a social category attributed to that person. Represents the CIDOC-CRM path: E21_Person > P107i_is_current_or_former_member_of > E74_Group. This captures social class, occupational groups, professional associations, or other social categorizations. This property is provided as a convenience for data entry and should be transformed to the full CIDOC-CRM structure for formal compliance. The range is restricted to gmn:E74_2_Social_Category groups."@en ;
    rdfs:subPropertyOf cidoc:P107i_is_current_or_former_member_of ;
    rdfs:domain cidoc:E21_Person ;
    rdfs:range gmn:E74_2_Social_Category ;
    dcterms:created "2025-10-16"^^xsd:date ;
    rdfs:seeAlso cidoc:P107i_is_current_or_former_member_of .

# Property: P107i.3 has occupation
gmn:P107i_3_has_occupation
    a owl:ObjectProperty ;
    a rdf:Property ;
    rdfs:label "P107i.3 has occupation"@en ;
    rdfs:comment "Simplified property associating a person with an occupational or professional group. Represents the CIDOC-CRM path: E21_Person > P107i_is_current_or_former_member_of > E74_Group. This captures professional affiliations and occupations by indicating membership in professional groups (notaries, merchants, bankers, etc.). This property is provided as a convenience for data entry and should be transformed to the full CIDOC-CRM structure for formal compliance. The range is restricted to gmn:E74_3_Occupational_Group groups."@en ;
    rdfs:subPropertyOf cidoc:P107i_is_current_or_former_member_of ;
    rdfs:domain cidoc:E21_Person ;
    rdfs:range gmn:E74_3_Occupational_Group ;
    dcterms:created "2025-10-16"^^xsd:date ;
    rdfs:seeAlso cidoc:P107i_is_current_or_former_member_of .

# Custom property for implicit typing
gmn:hasImplicitType
    a owl:AnnotationProperty ;
    rdfs:label "has implicit type"@en ;
    rdfs:comment "Indicates a type that should be automatically assigned to the target entity when transforming shortcut properties to full CIDOC-CRM structure. For example, when a property has gmn:hasImplicitType pointing to an AAT URI, that URI should be used as the P2_has_type value on the intermediate entity (E41_Appellation, E33_Linguistic_Object, etc.) created during transformation."@en .

# Documentation note
gmn:TransformationNote
    a rdfs:Resource ;
    rdfs:label "Transformation Documentation"@en ;
    rdfs:comment """
    Shortcut properties in this ontology (such as gmn:P1_1_has_name) are designed 
    for simplified data entry. When exporting data for interoperability or formal 
    CIDOC-CRM compliance, these properties should be transformed to their full 
    CIDOC-CRM equivalents.
    
    Example transformation for gmn:P1_1_has_name:
    
    INPUT (shortcut):
    <person> gmn:P1_1_has_name "Giovanni Battista" .
    
    OUTPUT (CIDOC-CRM compliant):
    <person> cidoc:P1_is_identified_by <appellation1> .
    <appellation1> a cidoc:E41_Appellation ;
                   cidoc:P2_has_type <http://vocab.getty.edu/page/aat/300458798> ;
                   cidoc:P190_has_symbolic_content "Giovanni Battista" .
    
    Note: The P2_has_type value (AAT 300458798 - personal names) is automatically 
    applied during transformation as specified by gmn:hasImplicitType.
    
    Example transformation for gmn:P1_2_has_name_from_source:
    
    INPUT (shortcut):
    <person> gmn:P1_2_has_name_from_source "Iohannes Baptista" .
    
    OUTPUT (CIDOC-CRM compliant):
    <person> cidoc:P1_is_identified_by <appellation2> .
    <appellation2> a cidoc:E41_Appellation ;
                   cidoc:P2_has_type <http://vocab.getty.edu/page/aat/300456607> ;
                   cidoc:P190_has_symbolic_content "Iohannes Baptista" .
    
    Note: The P2_has_type value (AAT 300456607 - names found in sources) is automatically 
    applied during transformation as specified by gmn:hasImplicitType.
    
    Example transformation for gmn:P3_1_has_editorial_note:
    
    INPUT (shortcut):
    <entity> gmn:P3_1_has_editorial_note "Need to verify this date against archival sources." .
    
    OUTPUT (CIDOC-CRM compliant):
    <entity> cidoc:P67i_is_referred_to_by <linguistic_object1> .
    <linguistic_object1> a cidoc:E33_Linguistic_Object ;
                         cidoc:P2_has_type <http://vocab.getty.edu/page/aat/300456627> ;
                         cidoc:P190_has_symbolic_content "Need to verify this date against archival sources." .
    
    Note: The P2_has_type value (AAT 300456627) is automatically applied during transformation 
    as specified by gmn:hasImplicitType. This property is marked as internal-only and may be 
    excluded from public exports.
    
    Example usage for gmn:P2_1_gender:
    
    INPUT (simplified):
    <person> gmn:P2_1_gender <http://vocab.getty.edu/page/aat/300189557> .
    
    OUTPUT (CIDOC-CRM compliant - same structure, as this is already a proper relationship):
    <person> cidoc:P2_has_type <http://vocab.getty.edu/page/aat/300189557> .
    
    Note: gmn:P2_1_gender is a subproperty of cidoc:P2_has_type with a restricted range 
    (male, female, intersex from Getty AAT). No transformation is needed as this property 
    directly represents the CIDOC-CRM structure, just with added semantic specificity.
    
    Example transformation for gmn:P1_4_has_loconym:
    
    INPUT (shortcut):
    <person> gmn:P1_4_has_loconym <http://example.com/places/genoa> .
    
    OUTPUT (CIDOC-CRM compliant):
    <person> cidoc:P1_is_identified_by <appellation1> .
    <appellation1> a cidoc:E41_Appellation ;
                   cidoc:P2_has_type <https://www.wikidata.org/wiki/Q17143070> ;
                   cidoc:P67_refers_to <http://example.com/places/genoa> .
    
    Note: When transforming gmn:P1_4_has_loconym, an E41_Appellation is created that references 
    the place via P67_refers_to. The appellation type is automatically set to Wikidata Q17143070 
    (loconym) as specified by gmn:hasImplicitType. If a person already has name appellations, 
    this should ideally be integrated with or linked to the existing name structure.
    
    Example transformation for gmn:P107i_1_has_regional_provenance:
    
    INPUT (shortcut):
    <person> gmn:P107i_1_has_regional_provenance <http://example.com/groups/republic_of_genoa> .
    
    OUTPUT (CIDOC-CRM compliant):
    <person> cidoc:P107i_is_current_or_former_member_of <http://example.com/groups/republic_of_genoa> .
    <http://example.com/groups/republic_of_genoa> a cidoc:E74_Group ;
                                                   cidoc:P2_has_type <http://vocab.getty.edu/page/aat/300055490> .
    
    Note: When transforming gmn:P107i_1_has_regional_provenance, the group is typed with 
    AAT 300055490 (regions (geographic)) as specified by gmn:hasImplicitType. If the target 
    group already exists with a different type, the regional type should be added rather than 
    replacing existing types.
    
    Example transformation for gmn:P107i_2_has_social_category:
    
    INPUT (shortcut):
    <person> gmn:P107i_2_has_social_category <http://example.com/groups/merchants> .
    
    OUTPUT (CIDOC-CRM compliant):
    <person> cidoc:P107i_is_current_or_former_member_of <http://example.com/groups/merchants> .
    <http://example.com/groups/merchants> a cidoc:E74_Group .
    
    Note: gmn:P107i_2_has_social_category transforms directly to cidoc:P107i_is_current_or_former_member_of 
    without adding an implicit type, allowing flexibility in how social categories are typed. 
    Social groups can be typed with appropriate AAT terms (e.g., AAT 300025520 for merchants, 
    AAT 300151202 for nobility) as separate assertions.
    
    Example usage for gmn:E74_1_Regional_Provenance:
    
    <http://example.com/groups/citizens_of_genoa> a gmn:E74_1_Regional_Provenance ;
        gmn:P1_1_has_name "Citizens of Genoa" ;
        cidoc:P74_has_current_or_former_residence <http://example.com/places/genoa> .
    
    <person1> cidoc:P107i_is_current_or_former_member_of <http://example.com/groups/citizens_of_genoa> .
    
    Note: gmn:E74_1_Regional_Provenance is a subclass of cidoc:E74_Group and inherits all 
    properties that apply to groups, including P1, P3, and P74. This class provides semantic 
    clarity that the group represents a regional or geographic affiliation.
    
    Example usage for gmn:E74_2_Social_Category:
    
    <http://example.com/groups/merchants> a gmn:E74_2_Social_Category ;
        gmn:P1_1_has_name "Merchants" ;
        cidoc:P2_has_type <http://vocab.getty.edu/page/aat/300025520> . # merchants (people)
    
    <person1> gmn:P107i_2_has_social_category <http://example.com/groups/merchants> .
    
    Note: gmn:E74_2_Social_Category is a subclass of cidoc:E74_Group and inherits all 
    properties that apply to groups. This class provides semantic clarity that the group 
    represents a social or occupational categorization rather than geographic or kinship groups.
    
    Example usage for gmn:E74_3_Occupational_Group:
    
    <http://example.com/groups/notaries> a gmn:E74_3_Occupational_Group ;
        gmn:P1_1_has_name "Notaries" ;
        cidoc:P2_has_type <http://vocab.getty.edu/page/aat/300025066> . # notaries (people)
    
    <person1> gmn:P107i_3_has_occupation <http://example.com/groups/notaries> .
    
    Note: gmn:E74_3_Occupational_Group is a subclass of cidoc:E74_Group and inherits all 
    properties that apply to groups. This class provides semantic clarity that the group 
    represents a professional or occupational affiliation.
    
    Example transformation for gmn:P11i_1_earliest_attestation_date:
    
    INPUT (shortcut):
    <person> gmn:P11i_1_earliest_attestation_date "1427-01-01"^^xsd:date .
    
    OUTPUT (CIDOC-CRM compliant):
    <person> cidoc:P11i_participated_in <event1> .
    <event1> a cidoc:E5_Event ;
             cidoc:P4_has_time-span <timespan1> .
    <timespan1> a cidoc:E52_Time-Span ;
                cidoc:P82a_begin_of_the_begin "1427-01-01"^^xsd:date .
    
